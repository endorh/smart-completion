{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${version}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthor}"
  ],
  "contact": {
    "homepage": "${modPage}",
    "sources": "https://github.com/${githubRepo}",
    "issues": "${modIssueTracker}"
  },
  "license": "${modLicense}",
  "icon": "icon.png",


  "environment": "client",
  "entrypoints": {
    "client": [
      "endorh.smartcompletion.fabric.SmartCompletionModFabric"
    ]
  },
  "mixins": [
    "smartcompletion.mixins.json",
    "smartcompletion-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "${fabricMinecraftVersion}"
  }
}